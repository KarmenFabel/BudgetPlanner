<Window x:Class="BudgetPlanner.ExpensesAll"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BudgetPlanner"
        mc:Ignorable="d"
        Title="ExpensesAll" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="AllEx" Margin="158,96,158,96" SelectionChanged="AllEx_SelectionChanged" AutomationProperties.IsOffscreenBehavior="Onscreen">
            <ListBoxItem IsSelected="True"/>
        </ListBox>
        <Label Content="Monthly Expenses" HorizontalAlignment="Center" Margin="0,28,0,0" VerticalAlignment="Top" FontSize="24" FontWeight="Bold"/>
        <Button x:Name="Cont" Content="Continue" HorizontalAlignment="Left" Margin="390,377,0,0" VerticalAlignment="Top" Click="Cont_Click"/>
    </Grid>
</Window>
